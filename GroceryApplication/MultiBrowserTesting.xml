<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Multibrowsertesting" parallel="tests">
  <test thread-count="5" name="ChromeTest">
  <parameter name="Browser" value="chrome"></parameter>
    <classes>
    <class name="testScript.LoginTestclass"/>
      <class name="testScript.ManageCategoryTest"/>
      <class name="testScript.ManageContactTest"/>
      <class name="testScript.ManageNewsTestclass"/>
      <class name="testScript.AdminUsersTest"/>
